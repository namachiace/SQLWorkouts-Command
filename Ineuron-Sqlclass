---create database command
create DATABASE Bigdatabootcamp;
create DATABASE bigtest1;
----- Get list of database:
show databases;
----- Drop the database:
DROP DATABASE bigtest1;
--- go inside particular DATABASE
use Bigdatabootcamp;
CREATE TABLE if NOT exists employee1
( emp_id int,
emp_Name VARCHAR(50));
---- list down all table under database
show create table employee1;
